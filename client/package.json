{
  "name": "react-rails",
  "version": "0.1.0",
  "description": "frontend",
  "author": "Nine and One",
  "private": true,
  "license": "Private",
  "scripts": {
    "clean:dev": "yarn rimraf ../public/webpack/development/*",
    "clean:build": "yarn rimraf ../public/webpack/production/*",
    "clean:prod": "yarn clean:build",
    "build:dev": "yarn clean:dev && cross-env NODE_ENV=development webpack -w",
    "build:prod": "yarn clean:prod && cross-env NODE_ENV=production webpack -p",
    "flow-typed-install": "yarn flow-typed install -s --ignoreDeps dev",
    "lint": "yarn lint-staged"
  },
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.0.0"
  },
  "cacheDirectories": [
    "client/node_modules"
  ],
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx}": [
        "yarn eslint --ext .jsx,.js src/ --fix",
        "git add ."
      ],
      "src/**/*.{css,scss}": [
        "yarn stylefmt",
        "git add ."
      ]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.38",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.1.1",
    "react-on-rails": "^10.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-transition-group": "2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "cxs": "^6.1.0",
    "workbox-webpack-plugin": "^3.0.0-alpha.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.38",
    "@babel/plugin-transform-classes": "^7.0.0-beta.38",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.38",
    "@babel/plugin-transform-spread": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-flow": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-0": "^7.0.0-beta.38",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "empty": "^0.10.1",
    "eslint": "^4.15.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "img-loader": "^2.0.0",
    "lint-staged": "^6.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.10",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.4.0",
    "stylelint-order": "^0.8.0",
    "webpack": "^3.10.0",
    "webpack-config": "^7.4.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
